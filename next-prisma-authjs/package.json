{
  "name": "barebones-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run prisma:generate && next build",
    "start": "next start",
    "lint": "next lint",
    "tsc": "tsc",
    "format": "prettier --write .",
    "prisma:generate": "dotenv -e .env.local -- prisma generate --schema src/lib/schema.prisma",
    "prisma:migrate": "dotenv -e .env.local -- prisma migrate dev --schema src/lib/schema.prisma"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.4.1",
    "@neondatabase/serverless": "^0.9.3",
    "@prisma/adapter-neon": "^5.17.0",
    "@prisma/client": "^5.17.0",
    "dotenv": "^16.4.5",
    "next": "14.2.4",
    "next-auth": "^5.0.0-beta.19",
    "react": "^18",
    "react-dom": "^18",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^9.0.8",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.4",
    "postcss": "^8.4.39",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "prisma": "^5.17.0",
    "tailwindcss": "^3.4.4",
    "typescript": "^5"
  }
}
